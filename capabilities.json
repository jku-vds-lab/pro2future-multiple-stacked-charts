                        "max": 100
                    },
                    "measure": {
                        "max": 100
                    },
                    "grouping": {
                        "max": 2
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "x_axis"
                            }
                        },
                        {
                            "for": {
                                "in": "y_axis"
