        "implicit": {
            "clauses": [
                {
                    "role": "x_axis",
                    "direction": 1
                }
            ]
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "x_axis": {
                        "max": 1
                    },
                    "y_axis": {
                        "max": 20
                    },
                    "overlayX": {
                        "max": 1
                    },
                    "overlayY": {
                        "max": 1
                    },
                    "legend": {
                        "max": 1
                    },
                    "filterLegend": {
